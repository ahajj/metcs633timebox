<mvc:View
	controllerName="com.metcs633.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="{i18n>title}">
		<content>
			<HBox>
				<Button id="signButton"
						text="Sign into Google"
						press="onSignInOutGooglePress">
				</Button>
				<Button text="Get Calendars"
						press="onGetCalendarsPress">
				</Button>
			</HBox>
			<Label id="configLabel"
					text="Not connected to Google...">
			</Label>
		</content>
	</Page>
</mvc:View>