{
  "name": "openui5-basic-template-app",
  "version": "0.1.0",
  "description": "Best-practice starting point for building a freestyle app",
  "private": true,
  "scripts": {
    "start": "npm-run-all comp-pre-js start-server",
    "start-server": "node ./bin/www",
    "comp-pre-js": "gulp ui5preload",
    "start-nm": "nodemon",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && rimraf coverage && npm run karma-ci",
    "build": "rimraf dist && ui5 build --a"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.0.0",
    "@openui5/sap.m": "^1.52.5",
    "@openui5/sap.ui.core": "^1.52.5",
    "@openui5/sap.ui.layout": "^1.52.5",
    "@openui5/themelib_sap_belize": "^1.52.5",
    "axios": "^0.19.2",
    "better-opn": "^1.0.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.0",
    "express": "~4.15.5",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.15",
    "morgan": "~1.9.0",
    "msal": "^1.2.1",
    "npm": "^6.13.7",
    "qs": "^6.9.1",
    "serve-favicon": "~2.4.5",
    "serve-index": "^1.9.1",
    "simple-oauth2": "^3.0.0"
  },
  "devDependencies": {
    "@ui5/cli": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-pretty-data": "^0.1.2",
    "gulp-uglify": "^3.0.2",
    "gulp-ui5-preload": "^1.3.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-ui5": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2"
  }
}